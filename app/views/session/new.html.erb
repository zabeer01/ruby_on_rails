<%= form_with url: sign_in_path do|form|%>
    <div class="form-group mb-3">
        <%= form.label :email %>
        <%= form.text_field :email, class: "form-control" %>
        </div>

        <div class="form-group mb-3">
        <%= form.label :password %>
        <%= form.password_field :password, class: "form-control" %>
        </div>

        <div class="form-group mb-3">
        <%= form.submit "Sign in", class: "btn btn-primary btn-sm" %>
    </div>

    <% end %>

